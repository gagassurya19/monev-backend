{
  "name": "monev-backend",
  "version": "1.0.0",
  "description": "Monev Backend - Sistem Monitoring dan Evaluasi dengan ETL, Logging, dan API Gateway",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "setup:db": "bash scripts/setup_database.sh",
    "db:backup": "bash scripts/backup_database.sh",
    "quick-setup": "bash scripts/quick-setup.sh",
    "migrate:tables": "bash scripts/migrate_tables.sh",
    "seed:admin": "node scripts/seed_admin_user.js",
    "seed:admin:super": "cross-env CREATE_SUPERADMIN=true node scripts/seed_admin_user.js"
  },
  "keywords": [
    "hapi",
    "mysql",
    "backend",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@hapi/boom": "^10.0.1",
    "@hapi/hapi": "^21.4.0",
    "@hapi/inert": "^7.1.0",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^7.0.3",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "faker": "^6.6.6",
    "hapi-cors": "^1.0.3",
    "hapi-swagger": "^17.3.2",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.4",
    "node-cron": "^4.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
